{
    "name": "moo/flashcard-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle that provides a simple flash card system with REST API.",
    "keywords": ["flash card", "knowledgebase"],
    "homepage": "https://github.com/satrun77/MooFlashCardBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Alsharaf",
            "email": "mohamed.alsharaf@gmail.com",
            "homepage": "http://my.geek.nz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.0",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "stof/doctrine-extensions-bundle": "1.1.*",
        "friendsofsymfony/rest-bundle": "1.1.*",
        "jms/serializer-bundle": "0.12.*",
        "nelmio/api-doc-bundle": "2.4.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "knplabs/knp-paginator-bundle": "~2.4"
    },
    "require-dev": {
        "symfony/finder": "~2.0",
        "symfony/form": "~2.3",
        "symfony/validator": "~2.1",
        "symfony/css-selector": "~2.3",
        "symfony/assetic-bundle": "2.3.*",
        "liip/functional-test-bundle": "~1.0",
        "doctrine/data-fixtures": "1.0.*",
        "doctrine/doctrine-fixtures-bundle": "~2.2"
    },
    "suggest": {
        "moo/flashcardadmin-bundle": "Provides admin classes that be used with SonataAdminBundle.",
        "doctrine/doctrine-fixtures-bundle": "Required when using the fixture loading functionality.",
        "doctrine/data-fixtures": "Required when using the fixture loading functionality.",
        "liip/functional-test-bundle": "Required for bundle unit testing.",
        "doctrine/migrations": "Required for database migration.",
        "doctrine/doctrine-migrations-bundle": "Required for database migration."
    },
    "autoload": {
        "psr-0": {"Moo\\FlashCardBundle": ""}
    },
    "target-dir": "Moo/FlashCardBundle"
}
